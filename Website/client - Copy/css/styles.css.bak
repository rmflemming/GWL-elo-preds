.top-header {
	background: #405275;
	height: 150px;
	display: flex;
	flex-direction: row;
}

.box {
	width: 33%;
	margin: auto;
	text-align: center;
}

.logo {
	float: right;
    max-height: 90px;
    max-width: 125px;
}

.logo-name {
    line-height: 85px;
    font-family: "Bitstream Vera Serif", serif !important;
    font-size: 44px;
    color: #fff;
}

.top-header-icons {
	display: flex;
	float: left;
	min-height: 32px;
    min-width: 32px;
}

.nav-item {
	font-size: 25px;
	font-family: "Bitstream Vera Serif", serif !important;
	color: #405275;
}

.top-team {
	margin-top: 20px;
}
#top-teams {
	margin-left: auto;
	margin-right: auto;
	width: 55%;
	text-align: center;
	border-collapse: collapse;
	height: 1px;
}

#top-teams th, td{
	padding: 0;
}
.iconContainer {
	width: 200px;
	height: 200px;
	text-align: center;
}
.parent {
  height: 200px;
  width: 200px;
  position: relative;
  top: 0;
  left: 0;
  text-align: center;
}

.top-icon {
	position: absolute;
	width: 200px;
	height: 200px;
	z-index: 1;
	text-align: center;
}

.top-icon-bg {
	position: relative;
	height: 200px;
	width: 200px;
	z-index: 0;
	opacity: 0.75;
}

.top-elo-icon {
	width: 45px;
	height: 45px;
}

.top-team-rank {
	font-weight: 500;
}
.top-team-name {
	font-weight: 500;
}
#top-team-header {
	font-size: 40px;
	font-weight: 350;
	text-align: left;
	border: none;
}
#teams-table {
	margin-left: auto;
	margin-right: auto;
	width: 55%;
}

#teams-table th, td {
	text-align: left;
}

#top-teams td {
	text-align: center;
}
.table-elo-icon {
	width: 32px;
	height: 32px;
}

.table-team-icon {
	width: 32px;
	height: 32px;
}

.trendDown {
	font-size: 14px;
	color: red;
}

.trendUp {
	font-size: 14px;
	color: green;
}

.bi-caret-up-fill {
	font-size: 18px;
}

.bi-caret-down-fill {
	font-size: 18px;
}

.noBorder {
  border: 0 !important;
}
